parameters:
    level: 6
    paths:
        - src
        - think
    scanFiles:
    stubFiles:
    reportUnmatchedIgnoredErrors: true
    ignoreErrors:
        - "#has parameter \\$\\w+? with no value type specified in iterable type array.$#"
        - "#(return)? type has no value type specified in iterable type array.$#"
includes:
    - phpstan-baseline.neon