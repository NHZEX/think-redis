{
    "name": "zxin/think-redis",
    "description": "think redis",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "require": {
        "php": "~8.0",
        "ext-json": "*",
        "ext-redis": "*",
        "topthink/framework": "~8.0.1"
    },
    "require-dev": {
        "php-cs-fixer/shim": "^3.14",
        "open-smf/connection-pool": "^1.0",
        "phpstan/phpstan": "^1.10.28",
        "phpunit/phpunit": "^9",
        "swoole/ide-helper": "^4.8",
        "symfony/var-dumper": "^6.0"
    },
    "suggest": {
        "open-smf/connection-pool": "Connection pool support"
    },
    "autoload": {
        "psr-4": {
            "Zxin\\Redis\\": "src/",
            "Zxin\\Think\\Redis\\": "think/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Zxin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "think": {
            "services": [
                "Zxin\\Think\\Redis\\Service"
            ],
            "config": {
                "redis": "config/redis.php"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": false
    },
    "minimum-stability": "stable"
}
